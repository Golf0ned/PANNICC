define i32 @fib_rec(i32 %0) {
entry:
  br i32 %0, label 1, label 10

1:
  %2 = sub i32 %0, i32 1
  br i32 %2, label 3, label 9

3:
  %4 = sub i32 %0, i32 1
  %5 = call i32 @fib_rec(i32 %4)
  %6 = sub i32 %0, i32 2
  %7 = call i32 @fib_rec(i32 %6)
  %8 = add i32 %5, i32 %7
  br label 11

9:
  br label 11

10:
  br label 11

11:
  %12 = phi i32 [ %8, %3 ], [ 1, %9 ], [ 0, %10 ]
  ret i32 %12
}

define i32 @fib_it(i32 %0) {
entry:
  br label 1

1:
  %2 = phi i32 [ %4, %5 ], [ 0, %entry ]
  %3 = phi i32 [ %7, %5 ], [ %0, %entry ]
  %4 = phi i32 [ %6, %5 ], [ 1, %entry ]
  br i32 %3, label 5, label 8

5:
  %6 = add i32 %2, i32 %4
  %7 = sub i32 %3, i32 1
  br label 1

8:
  ret i32 %2
}

define i32 @main() {
entry:
  %0 = call i32 @fib_rec(i32 10)
  %1 = call i32 @fib_it(i32 1000)
  %2 = add i32 %0, i32 %1
  ret i32 %2
}