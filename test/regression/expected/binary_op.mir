define i32 @add() {
entry:
  %0 = alloca i32
  %1 = alloca i32
  store i32 1, ptr %1
  %2 = alloca i32
  store i32 2, ptr %2
  %3 = alloca i32
  %4 = load ptr %1
  %5 = load ptr %2
  %6 = add i32 %4, i32 %5
  store i32 %6, ptr %3
  %7 = load ptr %3
  store i32 %7, ptr %0
  br label 8

8:
  %9 = load ptr %0
  ret i32 %9
}

