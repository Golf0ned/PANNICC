scope:
        pushq   %rbx    # [virtual]
        pushq   %rsp    # [virtual]
        pushq   %rbp    # [virtual]
        pushq   %r12    # [virtual]
        pushq   %r13    # [virtual]
        pushq   %r14    # [virtual]
        pushq   %r15    # [virtual]

.scope_entry:
        movq    $0, %rax
        popq    %rbx    # [virtual]
        popq    %rsp    # [virtual]
        popq    %rbp    # [virtual]
        popq    %r12    # [virtual]
        popq    %r13    # [virtual]
        popq    %r14    # [virtual]
        popq    %r15    # [virtual]
        ret

scope_reassign_variable:
