define i64 @simplify_cfg_delete_orphaned() {
entry:
  %0 = alloca i64
  br i64 1, label 1, label 2

1:
  store i64 2, ptr %0
  br label 4

2:
  store i64 3, ptr %0
  br label 4

3:
  br label 4

4:
  %5 = load ptr %0
  ret i64 %5
}

define i64 @simplify_cfg_merge_straight_line() {
entry:
  %0 = alloca i64
  %1 = alloca i64
  store i64 0, ptr %1
  br i64 1, label 2, label 7

2:
  br i64 2, label 3, label 4

3:
  store i64 3, ptr %0
  br label 10

4:
  %5 = load ptr %1
  %6 = add i64 %5, i64 4
  store i64 %6, ptr %1
  br label 8

7:
  store i64 5, ptr %0
  br label 10

8:
  %9 = load ptr %1
  store i64 %9, ptr %0
  br label 10

10:
  %11 = load ptr %0
  ret i64 %11
}

define i64 @simplify_cfg_delete_empty() {
entry:
  %0 = alloca i64
  br i64 1, label 1, label 5

1:
  br i64 2, label 2, label 3

2:
  br label 4

3:
  br label 4

4:
  br label 9

5:
  br i64 3, label 6, label 7

6:
  br label 8

7:
  br label 8

8:
  br label 9

9:
  store i64 4, ptr %0
  br label 10

10:
  %11 = load ptr %0
  ret i64 %11
}
